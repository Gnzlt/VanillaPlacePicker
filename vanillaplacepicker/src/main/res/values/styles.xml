<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">?attr/colorPrimary</item>
        <item name="colorPrimaryDark">?attr/colorPrimaryDark</item>
        <item name="colorAccent">?attr/colorAccent</item>
    </style>

    <!-- AppTheme NoActionBar Translucent StatusBar -->
    <style name="AppTheme.TranslucentStatus">
        <item name="android:windowTranslucentStatus">true</item> <!--Attribute support: After v-21-->
    </style>

</resources>
